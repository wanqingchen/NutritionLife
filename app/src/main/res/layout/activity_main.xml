<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:context="com.example.wanqingchen.nutritionlife.MainActivity"
            tools:ignore="DuplicateIds"
            tools:layout_editor_absoluteY="81dp">

            <TextView
                android:id="@+id/Date"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="13dp"
                android:text="TextView"
                android:textAlignment="center"
                android:textSize="24sp"
                app:layout_constraintBottom_toTopOf="@+id/imageButton7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="80dp"
                android:layout_height="80dp"

                android:layout_below="@+id/Date"
                android:layout_marginEnd="14dp"
                android:layout_toStartOf="@+id/imageButton3"
                android:onClick="btnFood_Click"
                android:scaleType="centerInside"
                app:layout_constraintBottom_toTopOf="@+id/textView22"
                app:layout_constraintEnd_toStartOf="@+id/imageButton4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/food" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_alignStart="@+id/Date"
                android:layout_alignTop="@+id/imageButton2"
                android:scaleType="centerInside"
                app:layout_constraintBottom_toTopOf="@+id/textView23"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageButton7"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/bloodglucose" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_below="@+id/Date"
                android:layout_toStartOf="@+id/imageButton7"
                android:scaleType="centerInside"
                app:layout_constraintBottom_toTopOf="@+id/textView24"
                app:layout_constraintEnd_toStartOf="@+id/imageButton7"
                app:layout_constraintStart_toEndOf="@+id/imageButton2"
                app:layout_constraintTop_toTopOf="@+id/imageButton2"
                app:srcCompat="@drawable/exercise" />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_below="@+id/Date"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/Date"
                android:scaleType="centerInside"
                app:layout_constraintBottom_toTopOf="@+id/textView21"
                app:layout_constraintEnd_toStartOf="@+id/imageButton3"
                app:layout_constraintStart_toEndOf="@+id/imageButton4"
                app:layout_constraintTop_toBottomOf="@+id/Date"
                app:srcCompat="@drawable/medicine" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/imageButton7"
                android:layout_below="@+id/imageButton7"
                android:text="Medicine"
                android:textAlignment="center"
                android:textColor="@android:color/darker_gray"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView25"
                app:layout_constraintStart_toEndOf="@+id/textView24"
                app:layout_constraintTop_toBottomOf="@+id/imageButton7" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/textView25"
                android:layout_below="@+id/textView25"
                android:layout_marginBottom="292dp"
                android:layout_marginEnd="1dp"
                android:text="Glucose"
                android:textAlignment="center"
                android:textColor="@android:color/darker_gray"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textView24"
                app:layout_constraintTop_toBottomOf="@+id/imageButton3" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_alignBaseline="@+id/textView22"
                android:layout_alignBottom="@+id/textView22"
                android:layout_toEndOf="@+id/imageButton2"
                android:text="Blood"
                android:textAlignment="center"
                android:textColor="@android:color/darker_gray"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textView21"
                app:layout_constraintTop_toBottomOf="@+id/imageButton3" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/imageButton4"
                android:layout_below="@+id/imageButton4"
                android:text="Exercise"
                android:textAlignment="center"
                android:textColor="@android:color/darker_gray"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/textView25"
                app:layout_constraintEnd_toStartOf="@+id/textView23"
                app:layout_constraintStart_toEndOf="@+id/textView22"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView21" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/imageButton2"
                android:layout_below="@+id/imageButton2"
                android:text="Food"
                android:textAlignment="center"
                android:textColor="@android:color/darker_gray"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView24"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageButton2" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView22"
                android:layout_below="@+id/textView25"
                android:layout_marginTop="39dp"
                android:text="BREAKFAST"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/breakfastItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView27"
                android:layout_below="@+id/textView27"
                android:text="TextView1" />

            <TextView
                android:id="@+id/lunch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/breakfastItem"
                android:layout_below="@+id/breakfastItem"
                android:text="LUNCH"
                android:textSize="18sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/dinner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/lunchItem"
                android:layout_below="@+id/lunchItem"
                android:text="DINNER"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lunchItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/lunch"
                android:layout_below="@+id/lunch"
                android:text="TextView2" />

            <TextView
                android:id="@+id/dinnerItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/dinner"
                android:layout_below="@+id/dinner"
                android:text="TextView3" />

            <TextView
                android:id="@+id/snack"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/dinnerItem"
                android:layout_below="@+id/dinnerItem"
                android:text="SNACK"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/snackItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/snack"
                android:layout_below="@+id/snack"
                android:text="TextView4" />

            <ImageButton
                android:id="@+id/imageButton10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView27"
                android:layout_below="@+id/snackItem"
                android:layout_marginTop="16dp"
                app:srcCompat="@drawable/questionmark" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/snackItem"
                android:layout_below="@+id/imageButton10"
                android:layout_marginStart="12dp"
                android:text="What's for dinner?" />

            <ImageButton
                android:id="@+id/imageButton11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textView30"
                android:layout_toEndOf="@+id/textView24"
                android:onClick="btnMyDay_Click"
                app:srcCompat="@drawable/sun" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView21"
                android:layout_below="@+id/imageButton11"
                android:text="My Day" />


        </RelativeLayout>
    </ScrollView>
</LinearLayout>